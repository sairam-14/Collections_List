package com.list;
import java.util.ArrayList;
public class Add_Method 
{
	public static void main(String[] args) 
	{
		ArrayList<Integer>a=new ArrayList<>(3);
		System.out.println(a);
		
		a.add(10);
		a.add(20);
		a.add(30);
		a.add(40);
		System.out.println(a);//10,20,30,40
		
		//================ADD WITH 2 PARAMETERS=====================
		
		a.add(1, 11);
		System.out.println(a);//10,11,20,30,40




	}

}
