package com.list;

import java.util.*;

public class Lsit_Iteratorss {

	public static void main(String[] args) {

		ArrayList<Integer> a1= new ArrayList<>();
		a1.add(1);
		a1.add(2);
		a1.add(3);
		ListIterator<Integer> i= a1.listIterator();
		while(i.hasNext()) {
			System.out.println(i.next());
		}
		
		while(i.hasPrevious()) {
			System.out.println(i.previous());
		}
	}

}
